        "# Function to find the trailing zeros in a Factorial of a Number\n",
        "def trailingZeroes(N):\n",
        "    \t#code here\n",
        "    \tres = 0\n",
        "    \ti = 5\n",
        "    \t\n",
        "    \twhile i <= N:\n",
        "    \t    res += N//i\n",
        "    \t    i *= 5\n",
        "    \treturn res\n",
        "\n",
        "# Test it with input\n",
        "trailingZeroes(251)\n",
        "\n",
        "# Output should be 62"
      ]
    }
